{
	"name": "gd-basic",
	"displayName": "gd-basic",
	"description": "Landuage support for GD-Basic",
	"publisher": "AndreasGrimm",
	"repository": {
		"url": "https://github.com/andreas-grimm/gd-basic.git",
		"type": "git"
	},
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.51.0"
	},
