10 REM IN-PLACE TRANSPOSE 3 X 3 MATRIX
20 OPTION BASE 0
30 DIM A(3,3)
40 REM LOAD DATA FOR A
50 DATA -2,2,-3,-1,1,3,2,0,-1
60 FOR I=0 TO 2
70 FOR J=0 TO 2
80 READ A(I,J)
90 NEXT J
100 NEXT I
110 PRINT "ORIGINAL 3 X 3 MATRIX"
120 GOSUB 240
130 REM COMPUTE DETERMINANT WITH LEIBNIZ FORMULA
140 LET Y=A(0,0)*A(1,1)*A(2,2)+A(0,1)*A(1,2)*A(2,0)+A(0,2)*A(1,0)*A(2,1)
150 LET Z=A(0,2)*A(1,1)*A(2,0)+A(0,1)*A(1,0)*A(2,2)+A(0,0)*A(1,2)*A(2,1)
160 LET D=Y-Z
170 PRINT "DETERMINANT:";D
230 GOTO 320
240 REM PRINT MATRIX
250 FOR I=0 TO 2
260 FOR J=0 TO 2
270 PRINT A(I,J),
280 NEXT J
290 PRINT
300 NEXT I
310 RETURN
320 END
